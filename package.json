    "yargs-parser": "^21.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/yargs": "^17.0.10",
    "chokidar-cli": "^3.0.0",
    "jest": "^27.5.1",
    "jest-pnp-resolver": "^1.2.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
